{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:prettier/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint", "prettier", "import"],
    "rules": {
        "prettier/prettier":["error", {
            "endOfLine": "auto"
          }],
        "import/extensions":"off",
        "no-console":"off"
    }
}
